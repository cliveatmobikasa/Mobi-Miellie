      <script mobi-async-script>
        (function() {
          let loadedScript = false;

          const triggerLoading = () => {
            if(!loadedScript) {
              loadedScript = true;
              document.dispatchEvent(new Event('loadscript'));
            }
          }
          window.addEventListener('click', triggerLoading, {once: true});
          window.addEventListener('touchstart', triggerLoading, {once: true});
          window.addEventListener('keydown', triggerLoading, {once: true});
          window.addEventListener('scroll', triggerLoading, {once: true});
          
          if(!loadedScript) {
            setTimeout(() => {
              loadedScript = true;
    
              document.dispatchEvent(new Event('loadscript'));
            }, 7000);
          }
